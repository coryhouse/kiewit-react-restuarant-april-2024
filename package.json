{
  "name": "kiewit-react-restaurant-april-2024",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently npm:start-app npm:start-api",
    "start-app": "vite --port 3000 --open --strictPort",
    "start-api": "json-server --watch db.json --port 3001",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "concurrently npm:start npm:playwright:codegen",
    "playwright:codegen": "playwright codegen"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.26.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jotai": "^2.8.0",
    "json-server": "^0.17.3",
    "ky": "^1.2.3",
    "msw": "^2.2.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.22.3",
    "valtio": "^1.13.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@tanstack/router-devtools": "^1.26.18",
    "@tanstack/router-vite-plugin": "^1.26.16",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
